<h1 class="header_of_page">Registered Users</h1>
<a href="/users/new">Signup!</a>
<% @users.each do |user| %>
  <h3>User: <a class="users" href="users/<%=user.id%>"><%=user.username%></a></h3>
  <ul>
    <li>Email: <%=user.email%></li>
    <li>Age: <%=user.age%></li>
    <li>Member Since: <%=user.created_at%></li>
  </ul>
<%end%>
